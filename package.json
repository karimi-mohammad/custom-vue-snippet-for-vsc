  "description": "A simple cutomed snippet for vue files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bard/my-project.git"
  },
  "contributes": {
